<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TabButton">
        <attr name="imageNormal" format="reference" />
        <attr name="imageSelected" format="reference" />
        <attr name="textColorNormal" format="color" />
        <attr name="textColorSelected" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="text" format="string" />
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="roundTextColor" format="color" />
        <attr name="roundTextSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="ImageViewButton">
        <attr name="imageResource" format="reference" />
        <attr name="txt" format="string" />
    </declare-styleable>

</resources>